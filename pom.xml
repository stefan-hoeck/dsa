      <dependency>
        <groupId>org.netbeans.api</groupId>
        <artifactId>org-netbeans-core-multiview</artifactId>
        <version>${netbeans.version}</version>
      </dependency>
    </dependencies>    
  </dependencyManagement>
  <build>
    <pluginManagement>
      <plugins>
        <plugin>
          <groupId>org.codehaus.mojo</groupId>
          <artifactId>nbm-maven-plugin</artifactId>
          <configuration>
            <brandingToken>${brandingToken}</brandingToken>
            <cluster>${brandingToken}</cluster>
            <useOSGiDependencies>true</useOSGiDependencies>
          </configuration>
        </plugin>
      </plugins>
    </pluginManagement>
  </build>

  <modules>
    <module>application</module>
    <module>branding</module>
    <module>world</module>
    <module>abilities</module>
    <module>abilitiesservices</module>
    <module>equipment</module>
    <module>equipmentservices</module>
    <module>being</module>
    <module>beingCalc</module>
    <module>beingServices</module>
    <module>beingUI</module>
    <module>generation</module>
    <module>rules</module>
  </modules>

  <properties>
    <scala.version>2.10.0</scala.version>
    <scalaz.version>7.0.0-M8</scalaz.version>
    <brandingToken>dsatool</brandingToken>
    <util.version>0.2.1-SNAPSHOT</util.version>
    <react.version>0.2.1-SNAPSHOT</react.version>
    <rpg.version>1.0.0-SNAPSHOT</rpg.version>
    <world.version>1.0.0-SNAPSHOT</world.version>
