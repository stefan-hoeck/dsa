        <artifactId>efa-core_2.10</artifactId>
        <version>${util.version}</version>
      </dependency>
      <dependency>
        <groupId>efa</groupId>
        <artifactId>efa-io_2.10</artifactId>
        <version>${util.version}</version>
      </dependency>
      <dependency>
        <groupId>efa</groupId>
        <artifactId>efa-nb_2.10</artifactId>
        <version>${util.version}</version>
      </dependency>
      <dependency>
        <groupId>efa</groupId>
        <artifactId>efa-localde_2.10</artifactId>
        <version>${util.version}</version>
      </dependency>

      <dependency>
        <groupId>efa.rpg</groupId>
        <artifactId>rpg-core</artifactId>
        <version>${rpg.version}</version>
