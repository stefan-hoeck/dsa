    <being.version>1.0.0-SNAPSHOT</being.version>
    <beingCalc.version>1.0.0-SNAPSHOT</beingCalc.version>
    <beingServices.version>1.0.0-SNAPSHOT</beingServices.version>
    <beingUI.version>1.0.0-SNAPSHOT</beingUI.version>
    <generation.version>1.0.0-SNAPSHOT</generation.version>
    <rules.version>1.0.0-SNAPSHOT</rules.version>
  </properties>
</project>

